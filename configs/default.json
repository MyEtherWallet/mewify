{
    "httpPort": 8545,
    "httpsPort": 8546,
    "ipc": {
        "win32": "\\\\.\\pipe\\geth.ipc",
        "darwin": "[[HOME_DIR]]/Library/Ethereum/geth.ipc",
        "linux": "[[HOME_DIR]]/.ethereum/geth.ipc"
    },
    "keystore": {
        "win32": "[[HOME_DIR]]\\mewify\\keys\\",
        "darwin": "[[HOME_DIR]]/mewify/keys/",
        "linux": "[[HOME_DIR]]/mewify/keys/"
    },
    "configDir": {
        "win32": "[[HOME_DIR]]\\mewify\\conf\\",
        "darwin": "[[HOME_DIR]]/mewify/conf/",
        "linux": "[[HOME_DIR]]/mewify/conf/"
    },
    "mewNodes": [{
        "name": "ETH",
        "url": "https://api.myetherapi.com/eth",
        "chainId": 1,
        "blockExplorerTX": "https://etherscan.io/tx/[[txHash]]",
        "blockExplorerAddr": "https://etherscan.io/address/[[address]]"
    }, {
        "name": "Ropsten",
        "url": "https://api.myetherapi.com/rop",
        "chainId": 3,
        "blockExplorerTX": "https://testnet.etherscan.io/tx/[[txHash]]",
        "blockExplorerAddr": "https://testnet.etherscan.io/address/[[address]]"
    }, {
        "name": "ETC",
        "url": "https://mewapi.epool.io",
        "chainId": 61,
        "blockExplorerTX": "https://gastracker.io/tx/[[txHash]]",
        "blockExplorerAddr": "https://gastracker.io/addr/[[address]]"
    }],
    "node": "{\"name\":\"ETH\",\"url\":\"https://api.myetherapi.com/eth\",\"chainId\":1,\"blockExplorerTX\":\"https://etherscan.io/tx/[[txHash]]\",\"blockExplorerAddr\":\"https://etherscan.io/address/[[address]]\"}",
    "rpcRateLimit": {
        "reporter": false,
        "reporterInterval": 5000,
        "serverInterval": 1000,
        "priorityMethods": [
            "eth_newBlockFilter"
        ]
    }
}
